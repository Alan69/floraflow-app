{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-jsx",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "paths": {
      "modules/*": ["modules/*"],
      "components/*": ["components/*"],
      "redux/*": ["redux/*"],
      "assets/*": ["assets/*"]
    }
  },
  "extends": "expo/tsconfig.base"
}
